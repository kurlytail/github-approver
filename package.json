{
    "name": "@kurlytail/github-approver",
    "description": "github-approver",
    "version": "0.0.1",
    "main": "dist/app.min.js",
    "devDependencies": {
        "nock": "10.0.6",
        "smee-client": "1.2.2",
        "probot": "9.15.1",
        "child_process": "1.0.2",
        "circular-dependency-plugin": "5.2.2",
        "eslint": "5.16.0",
        "eslint-config-prettier": "4.3.0",
        "eslint-import-resolver-babel-module": "5.3.1",
        "eslint-loader": "2.2.1",
        "eslint-plugin-babel": "5.3.1",
        "eslint-plugin-flowtype": "3.13.0",
        "eslint-plugin-import": "2.23.4",
        "eslint-plugin-jest": "22.21.0",
        "eslint-plugin-prettier": "3.4.0",
        "flow-bin": "0.95.1",
        "prettier": "1.19.1",
        "source-map-loader": "0.2.4",
        "uglifyjs-webpack-plugin": "2.2.0",
        "webpack": "4.46.0",
        "webpack-cli": "3.3.12",
        "webpack-node-externals": "1.7.2",
        "jest": "24.9.0",
        "jest-junit-reporter": "1.1.0",
        "jest-localstorage-mock": "2.4.14",
        "@babel/cli": "7.14.5",
        "@babel/core": "7.14.5",
        "@babel/plugin-proposal-class-properties": "7.14.5",
        "@babel/plugin-proposal-object-rest-spread": "7.14.5",
        "@babel/preset-env": "7.14.5",
        "@babel/preset-flow": "7.14.5",
        "@babel/register": "7.14.5",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "10.1.0",
        "babel-jest": "24.9.0",
        "babel-loader": "8.2.2",
        "babel-plugin-module-resolver": "3.2.0",
        "babel-preset-flow": "6.23.0"
    },
    "scripts": {
        "start": "probot run dist/app.min.js",
        "docs": "jsdoc -c jsdoc.json && npx opn-cli ./docs/index.html",
        "build": "NODE_ENV=\"debug\" webpack --mode development --display-modules --display-reasons",
        "lint": "eslint src",
        "test": "jest tests",
        "test:watch": "jest --watch tests",
        "test:ci": "jest --ci tests",
        "clean": "rimraf ./docs ./dist ./coverage test-report.xml"
    },
    "license": "UNLICENSED",
    "dependencies": {},

    "jest": {
        "testResultsProcessor": "./node_modules/jest-junit-reporter",
        "moduleDirectories": ["node_modules", "./src"],
        "testPathIgnorePatterns": ["./.generated"],
        "collectCoverage": true,
        "coverageReporters": ["text", "html"],
        "collectCoverageFrom": ["src/**/*.js", "!**/node_modules/**"],
        "setupFiles": ["jest-localstorage-mock"]
    }
}
